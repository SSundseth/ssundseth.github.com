---
layout: nil
---
<?xml version="1.0"?>
<feed xmlns="http://www.w3.org/2005/Atom">

	<title>Stefan Sundseth's Blog</title>
	<link href="http://ssundseth.github.com/"/>
	<link type="application/atom+xml" rel="self" href="http://ssundseth.github.com/atom.xml"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>http://ssundseth.github.com/</id>
	<author>
		<name>Stefan Sundseth</name>
		<email>ssundseth@gmail.com</email>
	</author>

	{% for post in site.posts %}
	  <entry>
    	<id>http://ssundseth.github.com{ post.id }}</id>
		<link type="text/html" rel="alternate" href="http://ssundseth.github.com{{ post.url }}"/>
		<title>{{ post.title }}</title>
		<published>{{ post.date | date_to_xmlschema }}</published>
		<updated>{{ post.date | date_to_xmlschema }}</updated>
		<author>
		  <name>Stefan Sundseth</name>
		  <uri>http://ssundseth.github.com/</uri>
		</author>
		<content type="html">{{ post.content | xml_escape }}</content>
	  </entry>
	{% endfor %}

</feed>
